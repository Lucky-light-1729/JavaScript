// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
void printFrequency(int arr[], int n) 
{
    int freq[100][2]={0}; 
    int size = 0; 
    for (int i = 0; i < n; i++) 
    {
        bool found = false;
        for (int j = 0; j < size; j++) 
        {
            if (freq[j][0] == arr[i]) 
            {
                freq[j][1]++;
                found = true;
                break;
            }
        }
        if (!found) 
        {
            freq[size][0] = arr[i];
            freq[size][1] = 1;
            size++;
        }
        
    }
    for (int i = 0; i < size; i++) {
        cout << freq[i][0] << "-->" << freq[i][1] 
             << " time" << (freq[i][1] > 1 ? "s" : "") << endl;
    }
}
int main() {
    cout << "size of array: ";
    int size;
    cin >> size;

    int* arr = new int[size]; 
    cout << "Enter array elements:\n";
    for (int i = 0; i < size; i++) 
    {
        cin >> arr[i];
    }
    printFrequency(arr,size) ;

    return 0;
}